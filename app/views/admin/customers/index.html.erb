<h1>会員一覧</h1>

  <thead>
    <tr>
       <th>会員ID</th>
       <th>氏名</th>
       <th>メールアドレス</th>
       <th>ステータス</th>
    </tr>
  </thead>
  
  <% @customers.each do |customer| %>
  <tbody>
    <tr>
     <td>
         <%= customer.id %>
     </td>
     <td><%= link_to admin_customer__path(item.id) do %>
         <%= customer.last_name %><%= customer.first_name %>
         <% end %>
     </td>
     <td>
         <%= customer.email %>
     </td>
     <td>
         <%= customer.is_deleted %>
     </td>
    </tr>
  <% end %>
  </tbody>